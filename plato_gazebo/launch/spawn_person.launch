<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="x" default="0.0"/>
  <arg name="y" default="0.0"/>
  <arg name="z" default="0.0"/>
  <arg name="yaw" default="0.0"/>
  <arg name="sdf_robot_file" default="$(find plato_gazebo)/models/my_box/model.sdf"/>
  <arg name="robot_name" default="my_box"/>

  <node name="$(arg robot_name)_spawn" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
        args="-file $(arg sdf_robot_file) -sdf -x $(arg x) -y $(arg y) -z $(arg z)  -R 0 -P 0 -Y $(arg yaw) -model $(arg robot_name)"/>
</launch>
